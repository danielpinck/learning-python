import random
liste = [False, False, False]
winner = True
liste[random.randint(0,2)] = winner
player = random.randint(0,2)

print(liste)
print(f"Spieler w채hlt index {player} mit Wert {liste[player]}")

if liste[0] is True and player == 0:
    print(f"1. Moderator w채hlt index {random.choice([1,2])}")
elif liste[1] is True and player == 1:
    print(f"1. Moderator w채hlt index {random.choice([0,2])}")
elif liste[2] is True and player == 2:
    print(f"1. Moderator w채hlt index {random.choice([0,1])}")






